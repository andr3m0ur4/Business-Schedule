{
  "name": "vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "chance": "^1.1.8",
    "core-js": "^3.8.3",
    "jquery": "^3.6.0",
    "moment": "^2.29.2",
    "popper.js": "^1.16.1",
    "tui-calendar": "^1.15.3",
    "tui-code-snippet": "^2.3.2",
    "tui-date-picker": "^4.3.1",
    "vue": "^3.2.13",
    "vue-ls": "^4.2.0",
    "vue-router": "^4.0.14",
    "vue-sweetalert2": "^5.0.2",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-undef": "warn",
      "no-empty": "warn",
      "no-unreachable": "warn",
      "no-debugger": "warn",
      "no-console": "warn",
      "no-unused-vars": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
